	name = Berker Peksag
	email = berker.peksag@gmail.com
[color]
	ui = auto
[core]
	filemode = false
	editor = nano
[status]
	showUntrackedFiles = no
[alias]
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
        st = status -unormal
[branch "master"]
	rebase = true
